{
    "name": "confetticode/error-handler",
    "description": "A great unified error handler for PHP",
    "keywords": ["error-handler"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Quynh Nguyen",
            "email": "seriquynh@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/log": "^3.0",
        "symfony/console": "7.0",
        "symfony/error-handler": "^7.0",
        "symfony/http-foundation": "^7.0"
    },
    "require-dev": {
        "monolog/monolog": "^3.0",
        "phpunit/phpunit": "10.0",
        "spatie/ignition": "^1.15",
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ConfettiCode\\ErrorHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ConfettiCode\\ErrorHandler\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit tests/ --colors=always"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
